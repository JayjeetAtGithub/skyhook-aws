[global]
thread
ioengine=libaio
direct=1
buffered=0
log_avg_msec=1000
group_reporting=1
rw=write
bs=8m
iodepth=100
numjobs=1
runtime=180
time_based
write_bw_log=write.results
write_iops_log=write.results
write_lat_log=write.results


[nvme1n1]
filename=/dev/nvme1n1

[nvme2n1]
filename=/dev/nvme2n1

[nvme3n1]
filename=/dev/nvme3n1

[nvme4n1]
filename=/dev/nvme4n1

[nvme5n1]
filename=/dev/nvme5n1

[nvme6n1]
filename=/dev/nvme6n1

[nvme7n1]
filename=/dev/nvme7n1

[nvme8n1]
filename=/dev/nvme8n1
